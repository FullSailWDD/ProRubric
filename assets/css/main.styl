@import 'jeet'

edit()

	
#sideNav
	col(1/7)
	span(1/7)
	background gray
	text-align center
	height 700px
	
ul
	padding-left 0
	
li
	list-style-type none
	
ul li a
	col(1/3)
	stack()
	padding 5% 0
	text-decoration none
	color white
	font-size 1.4em
	font-family arial
	
ul li a:hover
	background #424242
	
#mainDefaultBody
	col(6/7)
	span(6/7)
	background: -webkit-radial-gradient(red, green, blue); /* Safari 5.1 to 6.0 */
    background: -o-radial-gradient(red, green, blue); /* For Opera 11.6 to 12.0 */
    background: -moz-radial-gradient(red, green, blue); /* For Firefox 3.6 to 15 */
    background: radial-gradient(red, green, blue); /* Standard syntax (must be last) */
#rubricSearch
	center(500px)
	padding 2%
	
div input
	width 95%
	font-size 1.5em
	padding 2%
	background-color white
	
#rubricSearch button
	col(1/2)
	shift(1/2)
	margin-top 5%
	background-color gray
	font-size 1.5em	
	border none
	text-align center
	padding 10px 0
	
#rubricSearch button:hover
	background #424242
	
	